
# Pull the image from the docker hub
docker pull ubuntu: latest
#Login to the docker hub
docker login
#Tag the docker image to your registry
docker tag ubuntu:latest mamirucce/gitdockerl:v1
#Push the image to your registry
docker push mamirucce/gitdockerl:v1
#Pull the image from your registry
docker pull mamirucce/gitdockerl:v1
#run the image in your local
docker run -it -d mamirucce/gitdockerl!v1
